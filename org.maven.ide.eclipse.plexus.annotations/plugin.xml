<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.m2e.core.projectConfigurators">
      <configurator
            class="org.maven.ide.eclipse.plexus.annotations.internal.PlexusProjectConfigurator"
            id="org.maven.ide.eclipse.plexus.annotations.plexusConfigurator"
            name="Plexus Metadata Generation">
         <pluginExecutionFilter>
            <groupId>org.codehaus.plexus</groupId>
            <artifactId>plexus-component-metadata</artifactId>
            <versionRange>[1.0-beta-3.0.6,)</versionRange>
            <goals>
               <goal>generate-metadata</goal>
               <goal>generate-test-metadata</goal>
            </goals>
         </pluginExecutionFilter>
      </configurator>
   </extension>
</plugin>
